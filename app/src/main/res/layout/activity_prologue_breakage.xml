<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/prologue_breakage_game_background"
    tools:context="com.example.last_spring.gameprealpha.prologue.PrologueBreakage">

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="60dp"
        android:layout_marginEnd="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:orientation="vertical"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RadioGroup
            android:id="@+id/radioGroupPrologueBreakageID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="true"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="gone"
            tools:visibility="visible">

            <RadioButton
                android:id="@+id/buttonPrologueBreakageFirstID"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="#70ffffff"
                android:button="@drawable/radio_button_selector"
                android:onClick="onPrologueBreakageFirst"
                android:text="@string/prologue_breakage_button_game_over"
                android:textColor="#303030"
                android:textSize="16sp"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="355dp" />

            <RadioButton
                android:id="@+id/buttonPrologueBreakageSecondID"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="#70ffffff"
                android:button="@drawable/radio_button_selector"
                android:onClick="onPrologueMoveUp"
                android:text="@string/prologue_move_up"
                android:textColor="#303030"
                android:textSize="16sp"
                android:visibility="gone"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="387dp" />

            <RadioButton
                android:id="@+id/buttonPrologueBreakageThirdID"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#70ffffff"
                android:button="@drawable/radio_button_selector"
                android:onClick="onPrologueMoveDown"
                android:text="@string/prologue_move_down"
                android:textColor="#303030"
                android:textSize="16sp"
                android:visibility="gone"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="419dp" />

        </RadioGroup>

        <ScrollView
            android:id="@+id/scrollPrologueBreakageID"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/radioGroupPrologueBreakageID"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="40dp"
            android:background="#70ffffff"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="gone"
            tools:visibility="visible">

            <TextView
                android:id="@+id/textPrologueBreakageID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:lineSpacingExtra="3sp"
                android:lineSpacingMultiplier="0.8"
                android:orientation="vertical"
                android:text="@string/prologue_breakage_text_game_over"
                android:textColor="#303030"
                android:textSize="18sp"
                android:visibility="visible" />

        </ScrollView>

    </RelativeLayout>

    <Button
        android:id="@+id/buttonBreakageID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#90e9f7f7"
        android:onClick="onBreakage"
        android:text="@string/prologue_breakage_button_start"
        android:textColor="#303030"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.855" />

    <Button
        android:id="@+id/buttonBreakageTestID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#90e9f7f7"
        android:onClick="onBreakageTest"
        android:text="@string/prologue_breakage_button_test"
        android:textColor="#303030"

        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.969" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayoutFirstID"
        android:layout_width="109dp"
        android:layout_height="109dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.719">

        <ImageButton
            android:id="@+id/imageButtonBreakageFirstID"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@null"
            android:onClick="onButtonBreakageFirst"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/prologue_breakage_button_left" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayoutSecondID"
        android:layout_width="109dp"
        android:layout_height="106dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.939"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.719">

        <ImageButton
            android:id="@+id/imageButtonBreakageSecondID"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:adjustViewBounds="false"
            android:background="@null"
            android:cropToPadding="false"
            android:elevation="0dp"
            android:onClick="onButtonBreakageSecond"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.562"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.25"
            app:srcCompat="@drawable/prologue_breakage_button_right" />
    </android.support.constraint.ConstraintLayout>

    <TextView
        android:id="@+id/textFailCounterID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/hockeyapp_button_background"
        android:textSize="24sp"
        app:layout_constraintHorizontal_bias="0.063"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textTimeID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Время"
        android:textColor="@color/hockeyapp_button_background"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.018"
        app:layout_constraintHorizontal_bias="0.94" />

    <SeekBar
        android:id="@+id/seekBarBreakageID"
        android:layout_width="0dp"
        android:layout_height="22dp"
        android:background="@drawable/breakage_seekbar_background"
        android:max="400"
        android:progress="200"
        android:progressDrawable="@drawable/point"
        android:thumb="@drawable/breakage_thumb"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.612"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.186" />

    <RelativeLayout
        android:id="@+id/layoutPrologueTrainingBreakageID"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="80dp"
        android:layout_marginEnd="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="60dp"
        android:background="#96c7cf"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <LinearLayout
            android:id="@+id/linearLayoutTrainingPrologueFirst"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:orientation="vertical">

            <CheckBox
                android:id="@+id/checkBoxPrologueTrainingBreakageID"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_above="@+id/radioButtonTraining"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:paddingBottom="10dp"
                android:text="@string/prologue_training_button_training"
                android:textColor="#303030" />

            <RadioButton
                android:id="@+id/radioButtonTrainingBreakageID"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_above="@+id/layoutPrologueTrainingBackgroundID"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#96c7cf"
                android:button="@drawable/radio_button_selector"
                android:onClick="onPrologueStartTrainingBackpack"
                android:text="@string/prologue_training_button_close"
                android:textColor="#303030"
                android:textSize="16sp"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="419dp" />

        </LinearLayout>

        <ScrollView
            android:id="@+id/scrollPrologueTrainingID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/linearLayoutTrainingPrologueFirst"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:gravity="center"
            android:orientation="vertical"
            tools:visibility="visible">

            <TextView
                android:id="@+id/textPrologueTrainingBreakageID"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:lineSpacingExtra="3sp"
                android:lineSpacingMultiplier="0.8"
                android:orientation="vertical"
                android:text="@string/prologue_training_breakage_scene_text"
                android:textColor="#303030"
                android:textSize="18sp"
                android:visibility="visible" />

        </ScrollView>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/interfaceID"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageView
            android:id="@+id/imageBackgroundLuckTrueID"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="fitXY"
            android:visibility="gone"
            app:srcCompat="@drawable/background_luck_true" />

        <ImageView
            android:id="@+id/imageBackgroundLuckFalseID"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="fitXY"
            android:visibility="gone"
            app:srcCompat="@drawable/background_luck_false" />

        <ImageButton
            android:id="@+id/buttonMenuID"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="10dp"
            android:background="@null"
            android:onClick="onButtonMenu"
            android:scaleType="fitXY"
            android:visibility="gone"
            app:srcCompat="@drawable/button_menu" />

        <TextView
            android:id="@+id/textMessageID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="20dp"
            android:background="#96c7cf"
            android:text="@string/prologue_message"
            android:textColor="#303030"
            android:visibility="invisible" />

        <RelativeLayout
            android:id="@+id/menuMainID"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#40000000"
            android:visibility="gone">

            <RelativeLayout
                android:id="@+id/menuID"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="80dp"

                android:layout_marginEnd="40dp"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="40dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="60dp"
                android:background="@color/hockeyapp_button_background_pressed">

                <LinearLayout
                    android:id="@+id/layoutButtonsMenuID"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerInParent="true"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@color/hockeyapp_button_background_pressed"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/buttonMenuCancelD"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:background="#90e9f7f7"
                        android:onClick="onMenuCancelD"
                        android:text="@string/button_dialog_cancel"
                        android:textColor="#303030"
                        tools:text="@string/button_dialog_cancel" />

                    <Button
                        android:id="@+id/buttonMenuMenulD"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:background="#90e9f7f7"
                        android:onClick="onMenuMenu"
                        android:text="@string/button_dialog_menu"
                        android:textColor="#303030" />

                    <Button
                        android:id="@+id/buttonMenuLuckUplD"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:background="#90e9f7f7"
                        android:onClick="onMenuLuckUp"
                        android:text="@string/button_dialog_menu"
                        android:textColor="#303030"
                        android:visibility="gone" />

                    <Button
                        android:id="@+id/buttonMenuExitID"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:background="#90e9f7f7"
                        android:onClick="onMenuExit"
                        android:text="@string/button_dialog_exit"
                        android:textColor="#303030"
                        tools:text="@string/button_dialog_exit" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layoutFieldsMenuID"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_above="@+id/layoutButtonsMenuID"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@color/hockeyapp_button_background_pressed"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:weightSum="100">

                    <LinearLayout
                        android:id="@+id/layoutInventoryLineID"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginRight="5dp"
                        android:layout_weight="50"
                        android:background="@color/hockeyapp_button_background_pressed"
                        android:orientation="vertical"
                        android:weightSum="6">

                        <TextView
                            android:id="@+id/textInventoryTextID"
                            android:layout_width="match_parent"
                            android:layout_height="25dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="#6e9ba1"
                            android:gravity="center"
                            android:text="@string/inventory_text_main"
                            android:textAlignment="center"
                            android:textColor="@color/hockeyapp_background_white" />

                        <TextView
                            android:id="@+id/textInventoryFoodID"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="#90e9f7f7"
                            android:gravity="center"
                            android:onClick="onInventoryFood"
                            android:text="@string/inventory_food"
                            android:textColor="@color/hockeyapp_background_white" />

                        <TextView
                            android:id="@+id/textInventoryDrugsID"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="#90e9f7f7"
                            android:gravity="center"
                            android:onClick="onInventoryDrugs"
                            android:text="@string/inventory_drugs"
                            android:textColor="@color/hockeyapp_background_white" />


                        <TextView
                            android:id="@+id/textInventorySleepingBugID"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="@color/colorAccent"
                            android:gravity="center"
                            android:onClick="onInventorySleepingBug"
                            android:text="@string/inventory_sleeping_bug"
                            android:textColor="@color/hockeyapp_button_background_selected" />

                        <TextView
                            android:id="@+id/textInventoryRaincoatID"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="@color/colorAccent"
                            android:gravity="center"
                            android:onClick="onInventoryRaincoat"
                            android:text="@string/inventory_raincoat"
                            android:textColor="@color/hockeyapp_button_background_selected" />

                        <TextView
                            android:id="@+id/textInventoryKnifeID"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="@color/colorAccent"
                            android:gravity="center"
                            android:onClick="onInventoryKnife"
                            android:text="@string/inventory_knife"
                            android:textColor="@color/hockeyapp_button_background_selected"
                            tools:text="@string/inventory_knife" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/layoutStateLineID"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="5dp"
                        android:layout_weight="50"
                        android:background="@color/hockeyapp_button_background_pressed"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/layoutStateProgressBarsID"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@color/hockeyapp_button_background_pressed"
                            android:orientation="horizontal">

                            <ProgressBar
                                android:id="@+id/progressBarMenuHealthID"
                                style="?android:attr/progressBarStyleHorizontal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="30"
                                android:max="100"
                                android:onClick="onBarMenuHealth"
                                android:paddingBottom="5dp"
                                android:paddingLeft="5dp"
                                android:paddingRight="5dp"
                                android:paddingTop="5dp"
                                android:progress="50"
                                android:progressDrawable="@drawable/progress_bar_menu_vertical_normal" />

                            <ProgressBar
                                android:id="@+id/progressBarMenuHungerID"
                                style="?android:attr/progressBarStyleHorizontal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="30"
                                android:max="100"
                                android:onClick="onBarBarMenuHunger"
                                android:paddingBottom="5dp"
                                android:paddingLeft="5dp"
                                android:paddingRight="5dp"
                                android:paddingTop="5dp"
                                android:progress="50"
                                android:progressDrawable="@drawable/progress_bar_menu_vertical_normal" />

                            <ProgressBar
                                android:id="@+id/progressBarChapterTwoLuckID"
                                style="?android:attr/progressBarStyleHorizontal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="30"
                                android:max="100"
                                android:onClick="onBarMenuLuck"
                                android:paddingBottom="5dp"
                                android:paddingLeft="5dp"
                                android:paddingRight="5dp"
                                android:paddingTop="5dp"
                                android:progress="50"
                                android:progressDrawable="@drawable/progress_bar_menu_vertical_normal" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/layoutMenuImagesID"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="5"
                            android:background="@color/hockeyapp_button_background_pressed"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageHealthID"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="5dp"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_weight="1"
                                app:srcCompat="@drawable/menu_health" />

                            <ImageView
                                android:id="@+id/imageHungerID"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="5dp"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_weight="1"
                                app:srcCompat="@drawable/menu_hunger" />

                            <ImageView
                                android:id="@+id/imageLuckID"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="5dp"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_weight="1"
                                app:srcCompat="@drawable/menu_luck" />

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>
            </RelativeLayout>

        </RelativeLayout>

